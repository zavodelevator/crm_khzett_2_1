<p id="notice"><%= notice %></p>

<h1>Piples</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Parens name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Sex</th>
      <th>Beardth date</th>
      <th>Other date</th>
      <th>Customer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @piples.each do |piple| %>
      <tr>
        <td><%= piple.first_name %></td>
        <td><%= piple.last_name %></td>
        <td><%= piple.parens_name %></td>
        <td><%= piple.email %></td>
        <td><%= piple.phone %></td>
        <td><%= piple.sex %></td>
        <td><%= piple.beardth_date %></td>
        <td><%= piple.other_date %></td>
        <td><%= piple.customer_id %></td>
        <td><%= link_to 'Show', piple %></td>
        <td><%= link_to 'Edit', edit_piple_path(piple) %></td>
        <td><%= link_to 'Destroy', piple, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Piple', new_piple_path %>
